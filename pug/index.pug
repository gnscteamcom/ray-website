-
	var images = '/images'
	var files = '/files'
	var css = '/css'
	var js = '/js'

doctype html
html(lang='en')
	head
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1.0')
		meta(name='description', content='')
		meta(name='author', content='Ray Rinaldy')
		
		link(href='css/bootstrap.css', rel='stylesheet', type='text/css')
		link(href='css/font-awesome.css', rel='stylesheet', type='text/css')
		link(href='css/animate.css', rel='stylesheet')
		link(href='css/style.css', rel='stylesheet')
		link(href='css/default.css', rel='stylesheet')

		title Maple Music Entertainment

	body#page-top(data-spy='scroll', data-target='.navbar-custom')
		#preloader
			#load

		nav.navbar.navbar-custom.navbar-fixed-top(role='navigation')
			.container
				.navbar-header.page-scroll
					button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-main-collapse')
						i.fa.fa-bars
					a.navbar-brand(href='index.html')
						h1 MAPLE MUSIC

				.collapse.navbar-collapse.navbar-right.navbar-main-collapse
					ul.nav.navbar-nav
						li.active
							a(href='#intro') Home
						li
							a(href='#about') About
						li
							a(href='#service') Service
						li
							a(href='#contact') Contact

		section#intro.intro
			.slogan
				h2
					| WELCOME TO 
					span.text_color SQUAD
				h4 MUSIC FOR A MOMENT
			.page-scroll
				a.btn.btn-circle(href='#service')
					i.fa.fa-angle-double-down.animated

		section#about.home-section.text-center
			.heading-about
				.container
					.row
						.col-lg-8.col-lg-offset-2
							.wow.bounceInDown(data-wow-delay='0.4s')
								.section-heading
									h2 About us
									i.fa.fa-2x.fa-angle-down
			.container
				.row
					.col-lg-2.col-lg-offset-5
						hr.marginbot-50
				.row
					.col-md-3
						.wow.bounceInUp(data-wow-delay='0.2s')
							.team.boxed-grey
								.inner
									h5 Anna Hanaceck
									p.subtitle Pixel Crafter
									.avatar
										img.img-responsive.img-circle(src='img/team/1.jpg', alt='')
					.col-md-3
						.wow.bounceInUp(data-wow-delay='0.5s')
							.team.boxed-grey
								.inner
									h5 Maura Daniels
									p.subtitle Ruby on Rails
									.avatar
										img.img-responsive.img-circle(src='img/team/2.jpg', alt='')
					.col-md-3
						.wow.bounceInUp(data-wow-delay='0.8s')
							.team.boxed-grey
								.inner
									h5 Jack Briane
									p.subtitle jQuery Ninja
									.avatar
										img.img-responsive.img-circle(src='img/team/3.jpg', alt='')
					.col-md-3
						.wow.bounceInUp(data-wow-delay='1s')
							.team.boxed-grey
								.inner
									h5 Tom Petterson
									p.subtitle Typographer
									.avatar
										img.img-responsive.img-circle(src='img/team/4.jpg', alt='')

		section#service.home-section.text-center.bg-gray
			.heading-about
				.container
					.row
						.col-lg-8.col-lg-offset-2
							.wow.bounceInDown(data-wow-delay='0.4s')
								.section-heading
									h2 Our Services
									i.fa.fa-2x.fa-angle-down
			.container
				.row
					.col-lg-2.col-lg-offset-5
						hr.marginbot-50
				.row
					.col-md-3
						.wow.fadeInLeft(data-wow-delay='0.2s')
							.service-box
								.service-icon
									img(src='img/icons/service-icon-1.png', alt='')
								.service-desc
									h5 Print
									p
										| Vestibulum tincidunt enim in pharetra malesuada. Duis semper magna metus electram accommodare.
					.col-md-3
						.wow.fadeInUp(data-wow-delay='0.2s')
							.service-box
								.service-icon
									img(src='img/icons/service-icon-2.png', alt='')
								.service-desc
									h5 Web Design
									p
										| Vestibulum tincidunt enim in pharetra malesuada. Duis semper magna metus electram accommodare.
					.col-md-3
						.wow.fadeInUp(data-wow-delay='0.2s')
							.service-box
								.service-icon
									img(src='img/icons/service-icon-3.png', alt='')
								.service-desc
									h5 Photography
									p
										| Vestibulum tincidunt enim in pharetra malesuada. Duis semper magna metus electram accommodare.
					.col-md-3
						.wow.fadeInRight(data-wow-delay='0.2s')
							.service-box
								.service-icon
									img(src='img/icons/service-icon-4.png', alt='')
								.service-desc
									h5 Cloud System
									p
										| Vestibulum tincidunt enim in pharetra malesuada. Duis semper magna metus electram accommodare.

		section#contact.home-section.text-center
			.heading-contact
				.container
					.row
						.col-lg-8.col-lg-offset-2
							.wow.bounceInDown(data-wow-delay='0.4s')
								.section-heading
									h2 Get in touch
									i.fa.fa-2x.fa-angle-down
			.container
				.row
					.col-lg-2.col-lg-offset-5
						hr.marginbot-50
				.row
					.col-lg-8
						.boxed-grey
							#sendmessage Your message has been sent. Thank you!
							#errormessage
							form#contact-form.contactForm(action='', method='post', role='form')
								.row
									.col-md-6
										.form-group
											label(for='name')
												| Name
											input#name.form-control(type='text', name='name', placeholder='Your Name', data-rule='minlen:4', data-msg='Please enter at least 4 chars')
											.validation
										.form-group
											label(for='email')
												| Email Address
											.form-group
												input#email.form-control(type='email', name='email', placeholder='Your Email', data-rule='email', data-msg='Please enter a valid email')
												.validation
										.form-group
											label(for='subject')
												| Subject
											input#subject.form-control(type='text', name='subject', placeholder='Subject', data-rule='minlen:4', data-msg='Please enter at least 8 chars of subject')
											.validation
									.col-md-6
										.form-group
											label(for='name')
												| Message
											textarea.form-control(name='message', rows='5', data-rule='required', data-msg='Please write something for us', placeholder='Message')
											.validation
									.col-md-12
										button#btnContactUs.btn.btn-skin.pull-right(type='submit')
											| Send Message
					.col-lg-4
						.widget-contact
							h5 Main Office
							address
								strong Maple Music Entertainment
								br
								| 				  Sembawang
								br
								| 				  Singapore
								br
								abbr(title='Phone') Jovin Ardianto:
								|  +6584370107
								br
								abbr(title='Phone') Mary Ivanna Sunjoko:
								|  +6281286949287
							address
								strong Email
								br
								a(href='mailto:entertainmentmaple@gmail.com') entertainmentmaple@gmail.com
							address
								strong We're on social networks
								br
								ul.company-social
									li.social-facebook
										a(href='http://facebook.com/maplemusicsg', target='_blank')
											i.fa.fa-facebook
									li.social-twitter
										a(href='http://twitter.com/maplemusicsg', target='_blank')
											i.fa.fa-twitter
									li.social-instagram
										a(href='http://twitter.com/maplemusicsg', target='_blank')
											i.fa.fa-instagram

		footer
			.container
				.row
					.col-md-12.col-lg-12
						.wow.shake(data-wow-delay='0.4s')
							.page-scroll.marginbot-30
								a#totop.btn.btn-circle(href='#intro')
									i.fa.fa-angle-double-up.animated
